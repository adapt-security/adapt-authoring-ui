{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "properties": {
    "buildDir": {
      "description": "Location of UI build files",
    	"type": "string",
      "isDirectory": true,
      "default": "$TEMP/ui-build"
    },
    "srcDir": {
      "description": "Location of UI source files",
    	"type": "string",
      "isDirectory": true,
      "default": "$TEMP/ui-src"
    }
  }
}
